#!/bin/sh
cat << EOF | python manage.py shell
from django.contrib.auth import get_user_model

User = get_user_model()

User.objects.filter(username="momo").exists() or \
    User.objects.create_superuser("momo", "", "$MOMO_ADMIN_PASS")

User.objects.filter(username="vide").exists() or \
    User.objects.create_superuser("vide", "", "$VIDE_ADMIN_PASS")
EOF